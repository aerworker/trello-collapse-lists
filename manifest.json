   "background": {
        "scripts": ["background.js"]
   },
   "description": "Allows the user to collapse Trello lists. List setting is stored in Chrome extension storage.",
   "browser_action": {
      "default_icon": "icon.png"
   },
   "manifest_version": 2,
   "name": "Collapsible Trello Lists",
   "version": "1.1.3",
   "permissions": [
      "storage",
      "webNavigation",
      "*://trello.com/*"
   ]
}
