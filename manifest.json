{
   "content_scripts": [ {
      "css": [ "styles.css" ],
      "js": [ "script.js" ],
      "matches": [
        "https://trello.com/b/*"
      ]
   } ],
   "description": "Allows the user to collapse Trello columns. Column setting is stored in Chrome extension storage.",
   "browser_action": {
      "default_icon": "icon.png"
   },
   "manifest_version": 2,
   "name": "Collapsible Trello Columns",
   "version": "1.0",
   "permissions": [
      "storage",
      "https://trello.com/"
   ]
}
